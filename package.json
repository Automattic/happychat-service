{
  "name": "happychat-service",
  "version": "0.18.0",
  "description": "Socket.IO based chat server for happychat.",
  "main": "index.js",
  "scripts": {
    "postinstall": "sh ./bin/install",
    "test": "NODE_PATH=./src babel-node ./test/runner.js",
    "test:coverage": "NODE_PATH=./src babel-node ./test/runner.js --coverage",
    "test:min": "NODE_PATH=./src babel-node ./test/runner.js --reporter min",
    "test:watch": "nodemon -e js,jsx --exec npm run test:min",
    "build": "rm -rf lib; `npm bin`/babel -d lib ./src/",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/automattic/happychat-service.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/automattic/happychat-service/issues"
  },
  "homepage": "https://github.com/automattic/happychat-service#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "commander": "^2.9.0",
    "eslines": "^0.0.12",
    "eslint": "^3.13.0",
    "eslint-config-wpcalypso": "^0.6.0",
    "eslint-eslines": "0.0.1",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-wpcalypso": "^3.0.2",
    "glob": "^7.1.1",
    "istanbul": "^0.4.5",
    "mocha": "^2.4.5",
    "mocha-junit-reporter": "^1.12.0",
    "nodemon": "^1.11.0",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.4.8"
  },
  "peerDependencies": {
    "socket.io": "^1.0"
  },
  "dependencies": {
    "lodash": "^4.11.1",
    "ramda": "^0.22.1",
    "redux": "^3.5.2",
    "redux-delayed-dispatch": "^1.0.0-2",
    "simperium-jsondiff": "^1.0.1-1",
    "uuid": "^2.0.1"
  }
}
